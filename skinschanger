Section5:NewButton("Spawn House (equip hammer before executing)", "Spawns a house within the player's proximity.", function()
    local args = {
        [1] = game:GetService("Players").LocalPlayer.Character.Hammer,
        [2] = "Build",
        [3] = {
            ["Name"] = "SteelFence",
            ["Cframe"] = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.Angles(0,0,300)
        }
    }
    game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("UseItem"):FireServer(unpack(args))

    local args = {
        [1] = game:GetService("Players").LocalPlayer.Character.Hammer,
        [2] = "Build",
        [3] = {
            ["Name"] = "SteelFence",
            ["Cframe"] = CFrame.new(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position + game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * -5)
        }
    }
    game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("UseItem"):FireServer(unpack(args))

    local args = {
        [1] = game:GetService("Players").LocalPlayer.Character.Hammer,
        [2] = "Build",
        [3] = {
            ["Name"] = "SteelFence",
            ["Cframe"] = CFrame.new(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position + game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 5)
        }
    }
    game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("UseItem"):FireServer(unpack(args))

    local args = {
        [1] = game:GetService("Players").LocalPlayer.Character.Hammer,
        [2] = "Build",
        [3] = {
            ["Name"] = "SteelFence",
            ["Cframe"] = CFrame.new(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position + game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame.RightVector * 5)  * CFrame.fromEulerAnglesYXZ(300,0,0)
        }
    }
    game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("UseItem"):FireServer(unpack(args))

    local args = {
        [1] = game:GetService("Players").LocalPlayer.Character.Hammer,
        [2] = "Build",
        [3] = {
            ["Name"] = "SteelFence",
            ["Cframe"] = CFrame.new(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position + game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame.RightVector * -5) * CFrame.fromEulerAnglesYXZ(-300,0,0)
        }
    }
    game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("UseItem"):FireServer(unpack(args))
end)





local Section6 = Tab2:NewSection("Survivor Exploits")






Section6:NewButton("Loop punch all zombies (fists)", "Damages zombies within a reasonable distance.", function()
    for _, plr in pairs(workspace.Characters.Zombies:GetDescendants()) do
    local ohInstance1 = plr.Humanoid
    local ohNumber2 = 1

    game:GetService("ReplicatedStorage").Events.Survivor.PunchAttacked:FireServer(ohInstance1, ohNumber2)
    end
end)
Section6:NewButton("Zombie Immunity", "Protects you from zombie damage.", function()
    while task.wait(0.5) do
    game:GetService("ReplicatedStorage").Events.Survivor.PunchAttacked:FireServer()
    end
end)
Section6:NewButton("Remove Snow", "Removes snow.", function()
    game:GetService("Workspace").Camera.Snow:Destroy()
end)






local Section7 = Tab2:NewSection("Melee Smash Exploits")







Section7:NewButton("Damage everybody (FLAG ONLY)", "Damages players within a reasonable distance.", function()
    local ohInstance1 = workspace.Characters.Survivors.beatshack09["Head"]
    local ohTable2 = {
        ["HeadShot"] = true,
        ["Humanoid"] = workspace.Characters.Survivors.beatshack09.Humanoid,
        ["Combo"] = 3,
        ["Weapon"] = workspace.Characters.Survivors.ContPentruRafa.Flag,
        ["Player"] = game:GetService("Players").beatshack09,
        ["NoStruggle"] = true
    }

    workspace.Characters.Survivors.ContPentruRafa.Flag.Slash:FireServer(ohInstance1, ohTable2)
end)






local Tab3 = Window:NewTab("Gift Farming")
local Section8 = Tab3:NewSection("Gift Farming | Blatant")







Section8:NewButton("Teleport to Blue Gift", "Teleports the player to the blue gift.", function()
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Map["Gift of Kindness"].CFrame
end)
Section8:NewButton("Teleport to Red Gift", "Teleports the player to the red gift.", function()
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Map["Gift of Life"].CFrame
end)
Section8:NewButton("Teleport to Green Gift", "Teleports the player to the green gift.", function()
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Map["Gift of Fortune"].CFrame
end)







local Section9 = Tab3:NewSection("Gift Farming | Legit")









Section9:NewButton("Highlight Blue Gift", "Highlights the blue gift.", function()
    local gok = game:GetService("Workspace").Map["Gift of Kindness"]
    local hl = Instance.new("Highlight")
    hl.Parent = gok
    hl.Adornee = gok
    hl.DepthMode = Enum.DepthMode.AlwaysOnTop
    hl.Enabled = true
    hl.Color3 = Color3.fromRGB(255, 0, 0)
    hl.OutlineColor = Color3.fromRGB(255, 255, 255)
    hl.OutlineTransparency = 0
    hl.Archivable = true
end)
Section9:NewButton("Highlight Red Gift", "Highlights the red gift.", function()
    local gol = game:GetService("Workspace").Map["Gift of Life"]
    local hl = Instance.new("Highlight")
    hl.Parent = gol
    hl.Adornee = gol
    hl.DepthMode = Enum.DepthMode.AlwaysOnTop
    hl.Enabled = true
    hl.Color3 = Color3.fromRGB(255, 0, 0)
    hl.OutlineColor = Color3.fromRGB(255, 255, 255)
    hl.OutlineTransparency = 0
    hl.Archivable = true
end)
Section9:NewButton("Highlight Green Gift", "Highlights the green gift.", function()
    local gof = game:GetService("Workspace").Map["Gift of Fortune"]
    local hl = Instance.new("Highlight")
    hl.Parent = gof
    hl.Adornee = gof
    hl.DepthMode = Enum.DepthMode.AlwaysOnTop
    hl.Enabled = true
    hl.Color3 = Color3.fromRGB(255, 0, 0)
    hl.OutlineColor = Color3.fromRGB(255, 255, 255)
    hl.OutlineTransparency = 0
    hl.Archivable = true
end)








