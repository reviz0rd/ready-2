local Tab2 = Window:NewTab("Exploits")
local Section5 = Tab2:NewSection("Hammer Exploits")






Section5:NewButton("Spawn Platform Up (equip hammer before executing)", "Spawns a platform above the player's head.", function()
    local args = {
        [1] = game:GetService("Players").LocalPlayer.Character.Hammer,
        [2] = "Build",
        [3] = {
            ["Name"] = "SteelFence",
            ["Cframe"] = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.Angles(0,0,300)
        }
    }
    game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("UseItem"):FireServer(unpack(args))
end)


Section5:NewButton("Spawn Platform Forward (equip hammer before executing)", "Spawns a platform infront of the player's head.", function()
    local args = {
        [1] = game:GetService("Players").LocalPlayer.Character.Hammer,
        [2] = "Build",
        [3] = {
            ["Name"] = "SteelFence",
            ["Cframe"] = game:GetService("Players").LocalPlayer.Character["Left Leg"].CFrame * CFrame.Angles(0,0,300) * CFrame.new(7.75,0,-7.80)
        }
    }
    game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("UseItem"):FireServer(unpack(args))
end)


Section5:NewButton("Spawn Platform Down (equip hammer before executing)", "Spawns a platform below the player's head.", function()
    local args = {
        [1] = game:GetService("Players").LocalPlayer.Character.Hammer,
        [2] = "Build",
        [3] = {
            ["Name"] = "SteelFence",
            ["Cframe"] = game:GetService("Players").LocalPlayer.Character["Left Leg"].CFrame * CFrame.Angles(0,0,300) * CFrame.new(17.5,0,-7.8)
        }
    }
    game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("UseItem"):FireServer(unpack(args))
end)


